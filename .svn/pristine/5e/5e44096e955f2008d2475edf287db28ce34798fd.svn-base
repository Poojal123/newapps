<div class="container">
  <div class="row">
    <div class="auth-box">
	<div class="chart-wrapper mainspace">
	<a href><img src="assets/img/vara-logo.png"></a>
    
  <form  id="new_password-form" method="post" name="new_password-form" ng-submit="newPass()">

			<div class="form-group control-group">
			  <div class="controls">
				<input class="form-control" id="new_password" name="newPassword" placeholder="New Password" type="password" value="" ng-model="newPass.newPassword" required="required">
			  </div>
			</div>
           
		<div class="form-group control-group">
			  <div class="controls">
				<input class="form-control" id="confrimPassword" name="confrimPassword" placeholder="Confrim Password" type="password" value="" ng-model="newPass.confrimPassword" required="required">
			  </div>
			</div>

			<div class="form-group">
				<button type="submit" class="btn btn-info btn-fill btn-wd btn-od" name="newPass" style="width:100%">Submit</button>
			</div>
			
			
    
  </form>
  <br /><br /><br /><br />
 </div>
    </div>
  </div>
</div>